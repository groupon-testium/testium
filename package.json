{
  "name": "testium",
  "version": "3.3.1",
  "description": "Synchronous integration testing with Node.js bindings for WebDriver",
  "license": "BSD-3-Clause",
  "main": "lib/testium.js",
  "bin": "cli.js",
  "homepage": "https://github.com/groupon-testium/testium",
  "repository": {
    "type": "git",
    "url": "https://github.com/groupon/testium"
  },
  "bugs": {
    "url": "https://github.com/groupon/testium/issues"
  },
  "scripts": {
    "test": "make all",
    "posttest": "nlm verify",
    "watch": "coffee --no-header -wcbo lib src & nodemon -w lib -w test -e coffee,js,json -x \"mocha\""
  },
  "engines": {
    "node": ">=0.10.16"
  },
  "nlm": {
    "license": {
      "files": [
        "src"
      ]
    }
  },
  "dependencies": {
    "assertive": "^1.4.0",
    "debug": "^2.1.0",
    "lodash": "^4.6.1",
    "mkdirp": "~0.5.0",
    "selenium-download": "^2.0.0",
    "testium-core": "^1.3.0",
    "testium-driver-sync": "^2.1.0",
    "testium-mocha": "^1.0.0"
  },
  "devDependencies": {
    "assertive": "^2.1.0",
    "coffee-script": "^1.10.0",
    "mocha": "^3.1.2",
    "nlm": "^3.0.0",
    "nodemon": "^1.0.0",
    "rimraf": "^2.2.8",
    "testium-example-app": "^2.0.0"
  },
  "author": {
    "name": "Sean Massa",
    "email": "endangeredmassa@gmail.com"
  },
  "keywords": [
    "testing",
    "selenium",
    "webdriver"
  ],
  "files": [
    "*.js",
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
